
package main

import (
    "fmt"
    "os/exec"
)

func main() {
    // Command to list Docker containers
    cmd := exec.Command("sudo", "docker", "ps")

    // Execute the command
    output, err := cmd.Output()
    if err != nil {
        fmt.Println("Error executing command:", err)
        return
    }

    // Print the output
    fmt.Println(string(output))
}
