
package main

import (
	"fmt"
	"os/exec"
	"testing"
)

func TestConnectToRouter16(t *testing.T) {
	cmd := exec.Command("docker", "exec", "-i", "clab-frrlab-router2", "vtysh", "-c", "configure terminal")

	output, err := cmd.CombinedOutput()
	if err != nil {
		fmt.Println("Error:", err)
		fmt.Println("Output:", string(output))
		return
	}

	fmt.Println("Output:", string(output))
}
