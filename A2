
package main

import "os"

// GetRouterCredentials retrieves router credentials from environment variables.
func GetRouterCredentials() (username, password string, err error) {
    username = os.Getenv("ROUTER_USERNAME")
    password = os.Getenv("ROUTER_PASSWORD")
    if username == "" || password == "" {
        err = fmt.Errorf("Router credentials not provided")
    }
    return username, password, err
}
